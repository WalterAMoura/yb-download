{
  "name": "sleyersx/youtube-download",
  "description": "Sistema para uso pessoal para realizar o download de videos do YouTube.",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Walter Moura",
      "email": "walter.moura@wmourax.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\"  : "app/"
    }
  },
  "require": {
    "ext-pdo": "*",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "norkunas/youtube-dl-php": "dev-master"
  },
  "extra": {
    "symfony": {
      "endpoint": ["https://api.github.com/repos/web-token/recipes/contents/index.json?ref=main", "flex://defaults"]
    }
  }
}
